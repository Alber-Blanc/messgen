{
  "name": "messgenjs",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test-ui": "vitest --watch --ui",
    "prettier": "prettier --write  \"src/**/*.js\" \"tests/**/*.js\" \"*.js\"",
    "gen": "python3 ../../messgen-generate.py --basedir ../../tests/messages --protocol messgen/test_proto  --protocol another_proto --outdir ./tests",
    "gen-json": "npm run gen -- --lang json",
    "gen-ts": "npm run gen-json && npm run gen -- --lang ts",
    "gen-cpp": "npm run gen -- --lang cpp",
    "gen-md": "npm run gen -- --lang md",
    "generate-bit": "cd ../.. && python3 tests/python/generate_serialized_data.py",
    "benchmark": "vitest bench",
    "perf": "node --inspect=9229 --require ts-node/register ./src/index.ts",
    "coverage": "vitest run --coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "2.1.5",
    "prettier": "3.2.5",
    "tinybench": "^2.7.0",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.7.2",
    "vite": "5.4.11",
    "vitest": "2.1.5"
  }
}