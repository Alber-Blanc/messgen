{
  "name": "messgen",
  "version": "0.9.0",
  "repository": "https://github.com/Alber-Blanc/messgen",
  "scripts": {
    "build": "bunchee",
    "test": "vitest",
    "benchmark": "vitest bench",
    "coverage": "vitest run --coverage",
    "gen:exec": "python3 ../../messgen-generate.py",
    "gen": "npm run gen:exec -- --types ../../tests/msg/types --protocol ../../tests/msg/protocols:mynamespace/proto/test_proto --protocol ../../tests/msg/protocols:mynamespace/proto/subspace/another_proto --outdir ./tests",
    "gen:json": "npm run gen -- --lang json",
    "gen:ts": "npm run gen:json && npm run gen -- --lang ts",
    "gen:md": "npm run gen -- --lang md",
    "gen:fixtures:ts": "npm run gen:exec -- --lang ts --types ./tests/fixtures/types --outdir ./tests/fixtures/generated",
    "gen:fixtures:json": "npm run gen:exec -- --lang json --types ./tests/fixtures/types --outdir ./tests/fixtures/generated",
    "generate-bit": "cd ../.. && python3 tests/python/generate_serialized_data.py",
    "check:lint": "eslint --ext .ts src",
    "check:types": "npm run gen:ts && npm run gen:fixtures:ts && tsc --noEmit"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/crypto-js": "4.2.2",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "2.1.5",
    "bunchee": "6.3.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "prettier": "3.2.5",
    "tinybench": "^2.7.0",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "tsup": "8.3.5",
    "typescript": "5.7.2",
    "vite": "5.4.11",
    "vitest": "2.1.5"
  },
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ]
    }
  },
  "keywords": [
    "messgen",
    "messaging",
    "protocol"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "decimal.js": "10.5.0"
  }
}
